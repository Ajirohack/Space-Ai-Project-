    "axios": "^1.8.4",
    "concurrently": "^7.6.0",
    "express": "^5.1.0",
    "multer": "^1.4.5-lts.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "^5.0.1"
  }
}
